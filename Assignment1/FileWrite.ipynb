    "import time\n",
    "import datetime\n",
    "def writeLog(line):\n",
    "    ts = time.time()\n",
    "    st = datetime.datetime.fromtimestamp(ts).strftime('%Y-%m-%d %H:%M:%S')\n",
    "    file = open(\"logfile.txt\",\"a\") \n",
    "    file.write('\\n['+st+'] '+line) \n",
    "    file.close() "
   ]
